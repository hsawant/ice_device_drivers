<?xml version="1.0" encoding="utf-8"?>
<pb840_commandcfg xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="pb840_commandcfg.xsd">
  <serialportname>/dev/ttyUSB0</serialportname> <!-- example: /dev/ttyS0, /dev/ttyUSB0, COM1 -->
  <serialbaudrate>38400</serialbaudrate>
  <serialdatabits>8</serialdatabits>
  <serialstopbits>0</serialstopbits> <!-- 0 = 1 stop bit, 1 = 1.5 stop bit, 2 = 2 stop bits -->
  <serialparity>none</serialparity> <!-- none, even, odd, space, mark -->
  <serialhandshaking>none</serialhandshaking>
  <pbcommand>3</pbcommand> <!-- 2 = SNDA, 3 = SNDF -->
  <secondstorun>120</secondstorun>
</pb840_commandcfg>
